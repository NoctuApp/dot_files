# use colorls instead of ls
alias ls="colorls -a"
alias ll="colorls -la"

# Brew command
alias bu="brew update"
alias buu="brew update && brew upgrade"
alias bi="brew install"
alias bic="brew install --cask"
alias bs="brew search"
alias br="brew remove"

# Vi
alias vi="nvim"
alias vim="nvim"
alias n="nvim"
